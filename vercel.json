{
    "version": 2,
    "builds":[
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "srcsource": "(.*)",
            "destination": "./app.js"
        }
    ],
    "rewrites": [
        { 
            "src": "(.*)", 
            "destination": "./app.js" 
        }
    ]
}